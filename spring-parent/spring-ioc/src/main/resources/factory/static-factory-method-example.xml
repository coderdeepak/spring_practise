<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

	
	<bean id="admin" class="org.spring.ioc.basic.factorymethod.staticf.DepartmentFactory"
	factory-method="createDepartment">
	<constructor-arg value= "admin"/>
	</bean>
	
	
	<bean id="hr" class="org.spring.ioc.basic.factorymethod.staticf.DepartmentFactory"
	factory-method="createDepartment">
	<constructor-arg value= "HR"/>
	</bean>
</beans>	
